{
  "name": "openhab1-rest",
  "version": "0.0.0-development",
  "description": "node module that talks to the openhab1 rest interface",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "jest",
    "watch:test": "jest --watchAll",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bluebird": "3.5.0",
    "core-js": "2.4.1",
    "ramda": "0.24.1",
    "request": "2.81.0",
    "request-promise": "4.2.1",
    "rxjs": "5.4.2",
    "ws": "3.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.4",
    "@types/core-js": "0.9.36",
    "@types/jest": "20.0.2",
    "@types/node": "4.2.15",
    "@types/ramda": "0.24.3",
    "jest": "20.0.4",
    "semantic-release": "^6.3.6",
    "ts-jest": "20.0.7",
    "ts-node": "3.0.4",
    "tslint": "5.2.0",
    "typescript": "2.3.2",
    "url-parse": "1.1.9",
    "yargs": "8.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ds82/openhab1-rest.git"
  }
}
